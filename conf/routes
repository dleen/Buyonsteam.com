# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET	/						controllers.Application.index

GET	/games					controllers.Application.list(p:Int ?= 0, s:Int ?= 2, f ?= "")

GET	/reindex				controllers.Application.reindex
GET /ggins					controllers.Application.ggins

GET	/autocompleteSearch		controllers.Application.autocompleteSearch(term: String ?= "")

GET	/:name					controllers.Application.qlist(name: String)

# Map static resources from the /public folder to the /assets URL path
GET	/assets/*file			controllers.Assets.at(path="/public", file)
